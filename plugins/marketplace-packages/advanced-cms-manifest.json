{
    "package_info": {
        "name": "advanced-cms",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:30:55",
        "package_size": 24304,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "advanced-cms",
        "version": "1.0.0",
        "description": "Advanced content management with AI writing assistance, SEO optimization, multi-language support, and dynamic content personalization",
        "author": {
            "name": "Shopologic"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/core": "^1.0"
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\AdvancedCms\\": "src\/"
            }
        },
        "bootstrap": {
            "class": "AdvancedCmsPlugin",
            "file": "AdvancedCmsPlugin.php"
        },
        "permissions": [
            "hooks.content.*",
            "hooks.seo.*",
            "api.access",
            "dashboard.widget",
            "cron.execute",
            "content.create",
            "content.publish"
        ],
        "hooks": {
            "actions": [
                "content.created",
                "content.published",
                "seo.optimized",
                "content.personalized",
                "translation.generated"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "GET",
                    "path": "\/api\/v1\/cms\/content",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/cms\/content",
                    "handler": "handlePost"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/cms\/ai-generate",
                    "handler": "handlePost"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/cms\/seo-optimize",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/cms\/analytics",
                    "handler": "handleGet"
                }
            ]
        },
        "config": {
            "schema": {
                "configurable": {
                    "type": "boolean",
                    "default": true
                },
                "api_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "widget_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "features": [
                    "ai_writing",
                    "seo_optimization",
                    "multilingual",
                    "personalization"
                ],
                "content_types": [
                    "page",
                    "blog",
                    "product_description",
                    "landing_page"
                ]
            }
        },
        "scheduled_tasks": [
            "0 * * * * - optimize_content_seo",
            "0 2 * * * - generate_content_analytics",
            "0 *\/6 * * * - update_content_scores",
            "0 3 * * SUN - cleanup_old_revisions"
        ]
    },
    "marketplace_info": {
        "category": "general",
        "tags": [
            "general",
            "performance"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}