{
    "package_info": {
        "name": "advanced-personalization-engine",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:30:57",
        "package_size": 28094,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "advanced-personalization-engine",
        "version": "1.0.0",
        "description": "AI-powered personalization engine with real-time behavior analysis, dynamic content optimization, predictive recommendations, and omnichannel personalization",
        "author": {
            "name": "Shopologic"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/core": "^1.0"
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\AdvancedPersonalizationEngine\\": "src\/"
            }
        },
        "bootstrap": {
            "class": "AdvancedPersonalizationEnginePlugin",
            "file": "AdvancedPersonalizationEnginePlugin.php"
        },
        "permissions": [
            "hooks.personalization.*",
            "hooks.behavior.*",
            "api.access",
            "dashboard.widget",
            "cron.execute",
            "personalization.manage",
            "customer.profile"
        ],
        "hooks": {
            "actions": [
                "personalization.profile_updated",
                "behavior.event_tracked",
                "recommendation.generated",
                "content.personalized",
                "segment.customer_moved"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "GET",
                    "path": "\/api\/v1\/personalization\/profile\/{customer_id}",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/personalization\/recommendations",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/personalization\/content",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/behavior\/track",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/personalization\/analytics",
                    "handler": "handleGet"
                }
            ]
        },
        "config": {
            "schema": {
                "configurable": {
                    "type": "boolean",
                    "default": true
                },
                "api_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "widget_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "features": [
                    "real_time_personalization",
                    "behavioral_targeting",
                    "content_optimization",
                    "predictive_analytics"
                ],
                "ml_models": [
                    "collaborative_filtering",
                    "content_based",
                    "deep_learning",
                    "reinforcement_learning"
                ]
            }
        },
        "scheduled_tasks": [
            "*\/15 * * * * - process_behavioral_events",
            "0 1 * * * - update_customer_profiles",
            "0 2 * * * - train_recommendation_models",
            "0 3 * * * - optimize_content_performance"
        ]
    },
    "marketplace_info": {
        "category": "general",
        "tags": [
            "general",
            "real-time",
            "performance"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}