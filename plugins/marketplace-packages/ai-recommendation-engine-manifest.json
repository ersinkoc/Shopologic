{
    "package_info": {
        "name": "ai-recommendation-engine",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:30:57",
        "package_size": 25476,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "ai-recommendation-engine",
        "version": "1.0.0",
        "description": "Advanced AI-powered product recommendation engine with machine learning algorithms, collaborative filtering, and real-time personalization",
        "author": {
            "name": "Shopologic"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/analytics": "^1.0",
                "shopologic\/core": "^1.0"
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\AiRecommendationEngine\\": "src\/"
            }
        },
        "bootstrap": {
            "class": "AiRecommendationEnginePlugin",
            "file": "AiRecommendationEnginePlugin.php"
        },
        "permissions": [
            "hooks.order.*",
            "hooks.product.*",
            "hooks.customer.*",
            "api.access",
            "dashboard.widget",
            "cron.execute",
            "ai.recommendations.view",
            "ai.recommendations.manage",
            "ai.analytics.view"
        ],
        "hooks": {
            "actions": [
                "order.completed",
                "product.viewed",
                "cart.item_added",
                "customer.login",
                "product.price"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "GET",
                    "path": "\/api\/v1\/ai\/recommendations\/{customer_id}",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/ai\/track-interaction",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/ai\/similar-products\/{product_id}",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/ai\/train-model",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/ai\/analytics",
                    "handler": "handleGet"
                }
            ]
        },
        "config": {
            "schema": {
                "configurable": {
                    "type": "boolean",
                    "default": true
                },
                "api_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "widget_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "ml_training_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "real_time_processing": {
                    "type": "boolean",
                    "default": true
                }
            }
        },
        "scheduled_tasks": [
            "0 2 * * * - train_models",
            "*\/15 * * * * - process_interactions",
            "0 4 * * SUN - cleanup_old_data"
        ]
    },
    "marketplace_info": {
        "category": "ai",
        "tags": [
            "ai",
            "real-time"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}