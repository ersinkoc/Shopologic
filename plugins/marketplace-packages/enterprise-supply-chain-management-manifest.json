{
    "package_info": {
        "name": "enterprise-supply-chain-management",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:31:02",
        "package_size": 24480,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "enterprise-supply-chain-management",
        "version": "1.0.0",
        "description": "Advanced supply chain management with end-to-end visibility, supplier relationship management, logistics optimization, and blockchain traceability",
        "author": {
            "name": "Shopologic"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/core": "^1.0"
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\EnterpriseSupplyChainManagement\\": "src\/"
            }
        },
        "bootstrap": {
            "class": "EnterpriseSupplyChainManagementPlugin",
            "file": "EnterpriseSupplyChainManagementPlugin.php"
        },
        "permissions": [
            "hooks.supply_chain.*",
            "hooks.logistics.*",
            "api.access",
            "dashboard.widget",
            "cron.execute",
            "supply_chain.manage",
            "logistics.optimize"
        ],
        "hooks": {
            "actions": [
                "supply_chain.shipment_created",
                "supplier.performance_updated",
                "logistics.route_optimized",
                "supply_chain.disruption_detected",
                "blockchain.transaction_recorded"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "GET",
                    "path": "\/api\/v1\/supply-chain\/overview",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/suppliers\/evaluate",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/logistics\/optimize-routes",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/supply-chain\/trace-product",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/supply-chain\/risk-assessment",
                    "handler": "handleGet"
                }
            ]
        },
        "config": {
            "schema": {
                "configurable": {
                    "type": "boolean",
                    "default": true
                },
                "api_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "widget_enabled": {
                    "type": "boolean",
                    "default": true
                },
                "features": [
                    "supplier_management",
                    "logistics_optimization",
                    "traceability",
                    "risk_management"
                ],
                "integrations": [
                    "blockchain",
                    "iot_sensors",
                    "ai_optimization",
                    "third_party_logistics"
                ]
            }
        },
        "scheduled_tasks": [
            "0 *\/6 * * * - monitor_supply_chain_health",
            "0 2 * * * - optimize_logistics_routes",
            "0 3 * * * - update_supplier_performance",
            "0 4 * * * - assess_supply_chain_risks"
        ]
    },
    "marketplace_info": {
        "category": "ai",
        "tags": [
            "ai",
            "enterprise",
            "performance"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}