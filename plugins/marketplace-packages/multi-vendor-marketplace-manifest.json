{
    "package_info": {
        "name": "multi-vendor-marketplace",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:31:05",
        "package_size": 53826,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "Multi-Vendor Marketplace",
        "version": "1.0.0",
        "description": "Transform single store into multi-vendor marketplace with vendor management, commissions, and analytics",
        "author": {
            "name": "Shopologic Team",
            "email": "plugins@shopologic.com"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/commerce": "^1.0.0"
            }
        },
        "bootstrap": {
            "class": "MultiVendorMarketplacePlugin",
            "file": "MultiVendorMarketplacePlugin.php"
        },
        "permissions": [
            "vendors.manage",
            "products.read",
            "products.write",
            "orders.read",
            "commissions.write",
            "hooks.*"
        ],
        "hooks": {
            "actions": [
                "vendor.registered",
                "product.submitted",
                "order.completed",
                "commission.calculated",
                "admin.vendor.dashboard"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "POST",
                    "path": "\/api\/v1\/vendors\/register",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/vendors\/{id}\/products",
                    "handler": "handleGet"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/vendors\/{id}\/analytics",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/vendors\/commission-payout",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/marketplace\/stats",
                    "handler": "handleGet"
                }
            ]
        },
        "database": {
            "tables": [
                "vendors",
                "vendor_products",
                "vendor_commissions",
                "commission_payouts",
                "marketplace_analytics"
            ],
            "migrations": "migrations\/"
        },
        "assets": {
            "js": [
                "assets\/vendor-dashboard.js",
                "assets\/marketplace.js"
            ],
            "css": [
                "assets\/marketplace.css"
            ]
        },
        "config": {
            "schema": {
                "commission_rate": {
                    "type": "number",
                    "default": 15,
                    "min": 0,
                    "max": 50,
                    "description": "Default commission rate percentage"
                },
                "auto_approve_vendors": {
                    "type": "boolean",
                    "default": false,
                    "description": "Automatically approve new vendors"
                },
                "auto_approve_products": {
                    "type": "boolean",
                    "default": false,
                    "description": "Automatically approve vendor products"
                },
                "enable_vendor_dashboard": {
                    "type": "boolean",
                    "default": true,
                    "description": "Provide vendor dashboard"
                },
                "payout_frequency": {
                    "type": "select",
                    "default": "monthly",
                    "options": {
                        "weekly": "Weekly",
                        "monthly": "Monthly",
                        "quarterly": "Quarterly"
                    },
                    "description": "Commission payout frequency"
                }
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\MultiVendorMarketplace\\": "src\/"
            }
        }
    },
    "marketplace_info": {
        "category": "general",
        "tags": [
            "general",
            "analytics"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}