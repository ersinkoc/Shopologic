{
    "package_info": {
        "name": "smart-shipping",
        "version": "1.0.0",
        "package_date": "2025-06-30 19:31:13",
        "package_size": 16250,
        "package_format": "zip"
    },
    "plugin_info": {
        "name": "Smart Shipping Calculator",
        "version": "1.0.0",
        "description": "Intelligent shipping with real-time rates, delivery predictions, carbon footprint tracking, and optimization",
        "author": {
            "name": "Shopologic Team",
            "email": "plugins@shopologic.com"
        },
        "requirements": {
            "php": ">=8.3",
            "core": ">=1.0.0",
            "dependencies": {
                "shopologic\/commerce": "^1.0.0"
            }
        },
        "bootstrap": {
            "class": "SmartShippingPlugin",
            "file": "SmartShippingPlugin.php"
        },
        "permissions": [
            "orders.read",
            "shipping.calculate",
            "notifications.send",
            "hooks.*"
        ],
        "hooks": {
            "actions": [
                "checkout.shipping_methods",
                "order.shipped",
                "shipping.rate_calculated",
                "cart.updated"
            ]
        },
        "api": {
            "endpoints": [
                {
                    "method": "POST",
                    "path": "\/api\/v1\/shipping\/calculate",
                    "handler": "handlePost"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/shipping\/tracking\/{id}",
                    "handler": "handleGet"
                },
                {
                    "method": "GET",
                    "path": "\/api\/v1\/shipping\/carbon-footprint",
                    "handler": "handleGet"
                },
                {
                    "method": "POST",
                    "path": "\/api\/v1\/shipping\/optimize-routes",
                    "handler": "handlePost"
                }
            ]
        },
        "database": {
            "tables": [
                "shipping_zones",
                "shipping_rates",
                "delivery_predictions",
                "carbon_calculations",
                "shipping_analytics"
            ],
            "migrations": "migrations\/"
        },
        "assets": {
            "js": [
                "assets\/shipping-calculator.js",
                "assets\/delivery-tracker.js"
            ],
            "css": [
                "assets\/shipping.css"
            ]
        },
        "config": {
            "schema": {
                "enable_real_time_rates": {
                    "type": "boolean",
                    "default": true,
                    "description": "Calculate real-time shipping rates"
                },
                "show_delivery_prediction": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show estimated delivery dates"
                },
                "enable_carbon_tracking": {
                    "type": "boolean",
                    "default": true,
                    "description": "Track carbon footprint of shipments"
                },
                "free_shipping_threshold": {
                    "type": "number",
                    "default": 50,
                    "min": 0,
                    "max": 500,
                    "description": "Free shipping minimum order amount"
                },
                "enable_eco_shipping": {
                    "type": "boolean",
                    "default": true,
                    "description": "Offer eco-friendly shipping options"
                }
            }
        },
        "autoload": {
            "psr-4": {
                "Shopologic\\Plugins\\SmartShipping\\": "src\/"
            }
        }
    },
    "marketplace_info": {
        "category": "shipping",
        "tags": [
            "shipping",
            "real-time",
            "performance"
        ],
        "compatibility": {
            "php": ">=8.3",
            "shopologic": ">=2.0"
        },
        "pricing": {
            "model": "free",
            "price": 0
        }
    },
    "quality_metrics": {
        "health_score": 0,
        "performance_grade": "A",
        "test_coverage": 40,
        "security_score": 100
    },
    "assets": {
        "icon": "marketplace-assets\/icon.svg",
        "banner": "marketplace-assets\/banner.svg",
        "logo": "marketplace-assets\/logo.svg",
        "screenshots": [
            "marketplace-assets\/screenshots\/screenshot-1.svg",
            "marketplace-assets\/screenshots\/screenshot-2.svg",
            "marketplace-assets\/screenshots\/screenshot-3.svg"
        ]
    }
}